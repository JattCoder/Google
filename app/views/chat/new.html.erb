<%= form_tag account_menu_chat_new_path do %>
    <% if @error != nil%>
        <script>
            alert(<%= raw @error.to_json %>)
        </script>
    <% end %>
    <%= text_field_tag 'title',nil, placeholder: 'Title', type: "text" %><br>
    <%= text_field_tag 'message',nil, placeholder: 'Message', type: "text" %><br>
    <%= submit_tag "Start Chat",:id=>"start_btn", :style=>"margin-bottom" %>
<% end %>